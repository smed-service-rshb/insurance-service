				<textElement textAlignment="Justified" markup="styled">
					<font fontName="Arial" size="11" isBold="false"/>
				</textElement>
				<textFieldExpression><![CDATA["	2. Я уведомлен"]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true">
				<reportElement positionType="Float" x="0" y="17" width="555" height="17" uuid="fba87ba1-81b8-4ca6-9415-36bec0652f01">
